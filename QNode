
/**
 * This class contains the names and phone numbers of the wait listed people 
 * @author swaleha
 * @date 14/07/2017
 */
public class Node {

	private String name,phone;
	private Node next;
	
	public Node(String n,String p){
		this.name = n;
		this.phone = p;
	}
	
	public void setName(String n){
		this.name = n;
	}
	public void setPhone(String p){
		this.phone = p;
	}
	public void setNext(Node n){
		this.next = n;
	}
	public String getName(){
		return this.name;
	}
	public String getPhone(){
		return this.phone;
	}
	public Node getNext(){
		return this.next;
	}
	
}
