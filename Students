
public class Students {

	String name;
	int rollno, age;

	public Students() {
		System.out.println("Adding student records!");
	}

	public Students(String name, int rollno, int age) {
		this.name = name;
		this.rollno = rollno;
		this.age = age;
	}

	public static void main(String[] args) {

		Students s1 = new Students();
		s1.name = "Tom";
		s1.rollno = 50;
		s1.age = 15;

		Students s2 = new Students();
		s2.name = "Jerry";
		s2.rollno = 20;
		s2.age = 15;

		Students s3 = new Students("Winnie", 60, 15);

		System.out.println("\nParameterized constructor name:" + s3.name);
		System.out.println("Parameterized constructor rollno:" + s3.rollno);
		System.out.println("Parameterized constructor age:" + s3.age);
	}

}
