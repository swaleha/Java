
public class Node {

	private String name;
	private String usn;
	private int year;
	
	private Node next;
	
	public Node(String n,String u,int y){
		this.name = n;
		this.usn= u;
		this.year = y;
	}
	
	public void setName(String n){
		this.name = n;
	}
	
	public void setUSN(String u){
		this.usn = u;
	}
	
	public void setYear(int y){
		this.year = y;
	}
	
	public void setNext(Node n){
		this.next = n;
	}
	
	public String getName(){
		return this.name;
	}
	
	public String getUSN(){
		return this.usn;
	}
	
	public int getYear(){
		return this.year;
	}
	
	public Node getNext(){
		return this.next;
	}
}
